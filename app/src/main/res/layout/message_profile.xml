        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:padding="@dimen/message_padding">
            <ImageView

                android:id="@+id/ivContactItem10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:visibility="invisible"
                android:layout_marginRight="@dimen/message_padding"
                android:src="@drawable/ic_phone_black" />

            <TextView
                android:id="@+id/m_messagetitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_toEndOf="@id/ivContactItem10"
                android:layout_toRightOf="@id/ivContactItem10"
                android:text=""
                android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                android:textColor="@color/textColor" />

            <!--<TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
