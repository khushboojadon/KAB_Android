            android:clickable="true"
            android:padding="@dimen/message_padding">

            <ImageView
                android:id="@+id/ivContactItem5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"

                android:layout_marginRight="@dimen/message_padding"
                android:src="@drawable/ic_location_on_black" />

            <TextView
                android:id="@+id/m_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/message_padding"
                android:layout_toRightOf="@id/ivContactItem5"
                android:ellipsize="end"
                android:text=""
                android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                android:textColor="@color/textColor" />

            <!--<TextView
                android:layout_width="wrap_content"
